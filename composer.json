{
    "name": "larsnieuwenhuizen/clubhouse-connector",
    "description": "Service package to connect and interface with Clubhouse through php",
    "minimum-stability": "stable",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Lars Nieuwenhuizen",
            "email": "lars.nieuwenhuizen@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LarsNieuwenhuizen\\ClubhouseConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarsNieuwenhuizen\\ClubhouseConnector\\Tests\\": "tests/"
        }
